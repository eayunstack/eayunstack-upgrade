{
        "datacenter": "br_storage",
        "data_dir": "/var/lib/consul",
        "log_level": "INFO",
        "node_name": "<%= @host_name%>",
        <% if @eayunstack_node_role == 'controller'%>
        "server": true,
        "bootstrap_expect": <%= @bootstrap_expect%>,
        <% end %>
        "ui_dir": "/opt/dist",
        "bind_addr": "<%= @storage_ip%>",
        "client_addr": "<%= @storage_ip%>",
        "start_join": <%= @storage_ips%>,
        "enable_syslog": true
}

